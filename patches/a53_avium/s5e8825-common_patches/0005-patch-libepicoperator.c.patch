     name: "libepicoperator",
-    srcs: ["epicoperator.c"],
+    srcs: ["libepicoperator.c"],
     vendor: true,
 }
diff --git a/shims/libepicoperator/epicoperator.c b/shims/libepicoperator/epicoperator.c
